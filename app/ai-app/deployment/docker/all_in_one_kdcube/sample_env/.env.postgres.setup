# Postgres setup job (one-shot)
# Used by docker-compose service: postgres-setup

POSTGRES_USER="<POSTGRES_USER>"
POSTGRES_PASSWORD="<POSTGRES_PASSWORD>"
POSTGRES_DATABASE=kdcube
POSTGRES_PORT=5432
POSTGRES_SSL=False

# For Postgres container
POSTGRES_DB="${POSTGRES_DATABASE}"
PGPORT="${POSTGRES_PORT}"

# Project bootstrap (creates schema/tenant/project as needed)
TENANT_ID=default
PROJECT_ID=default

# Optional: override admin defaults used by the bootstrap script
# ADMIN_USER_ID=
# ADMIN_USER_EMAIL=
# ADMIN_USER_NAME=
